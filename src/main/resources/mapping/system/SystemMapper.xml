<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.dbke.mapper.system.SystemMapper">
	<select id="selectById" parameterType="string" resultType="ApplicationSystem">
		select * from t_system where id = #{id}
	</select>
	
	<select id="selectAll" parameterType="string" resultType="ApplicationSystem">
		select * from t_system
	</select>
</mapper>